@model MneLabMVC.Models.Entitys.PersonellerTBL
@{
    ViewBag.Title = "PersonelEkle";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
<!DOCTYPE html>
<html>


    <title>personel Ekle</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">


<body>
    <br />
    <br />
    <br />

    <form class="w3-container w3-card-4 w3-light-grey w3-text-blue w3-margin" method="post">
        <h2 class="w3-center">Personel Kayıt</h2>

        <div class="w3-row w3-section">

            <div class="w3-rest">
                <input class="w3-input w3-border" name="PersonelAd" type="text" placeholder="Adınız">
            </div>
        </div>

        <div class="w3-row w3-section">

            <div class="w3-rest">
                <input class="w3-input w3-border" name="PersonelSoyad" type="text" placeholder="Soyadınız">
            </div>
        </div>

        <div class="w3-row w3-section">

            <div class="w3-rest">
                <input class="w3-input w3-border" name="Mail" type="email" placeholder="Mail Adresiniz">
            </div>
        </div>

        <div class="w3-row w3-section">

            <div class="w3-rest">
                <input class="w3-input w3-border" name="KullanıcıAdi" type="text" placeholder="Kullanıcı Adınız">
            </div>
        </div>


        <div class="w3-row w3-section">

            <div class="w3-rest">
                <input class="w3-input w3-border" name="Sifre1" type="password" placeholder="Şifre Ekleyin">
            </div>
        </div>


        <div class="w3-row w3-section">

            <div class="w3-rest">
                <input class="w3-input w3-border" name="Sifre2" type="password" placeholder="Şifre Ekleyin">
            </div>
        </div>

      
       



        <button class="w3-button w3-block w3-section w3-blue w3-ripple w3-padding">Personel Ekle</button>

    </form>

</body>
</html>


